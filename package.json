{
  "name": "penchiserver",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "dev": "cross-env NODE_ENV=development npm-run-all --parallel watch:server watch:front",
    "watch:front": "webpack --watch --progress --debug",
    "watch:server": "nodemon --ignore front.component/ --ignore public/dist/",
    "build": "cross-env NODE_ENV=production webpack --progress --hide-modules"
  },
  "dependencies": {
    "body-parser": "~1.15.2",
    "cookie-parser": "~1.4.3",
    "debug": "~2.2.0",
    "express": "^4.15.2",
    "morgan": "~1.7.0",
    "pug": "^2.0.0-beta11",
    "serve-favicon": "~2.3.0",
    "vue": "^2.2.5"
  },
  "devDependencies": {
    "babel-core": "^6.0.0",
    "babel-loader": "^6.0.0",
    "babel-preset-latest": "^6.0.0",
    "cross-env": "^3.0.0",
    "css-loader": "^0.25.0",
    "file-loader": "^0.9.0",
    "nodemon": "^1.11.0",
    "npm-run-all": "^4.0.2",
    "vue-loader": "^11.1.4",
    "vue-template-compiler": "^2.2.1",
    "webpack": "^2.2.0",
    "webpack-dev-server": "^2.2.0"
  }
}